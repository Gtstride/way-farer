{
  "name": "way-farer",
  "version": "1.0.0",
  "description": "A transportation booking web app",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/nodemon app.js --exec babel-node --",
    "migrate": "babel-node ./server/models",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --require @babel/register --timeout 120000 --require @babel/polyfill server/test --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "showcoverage": "start ./coverage/index.html",
    "parser": "babel-eslint",
    "lint": "./node_modules/.bin/eslint server/test --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gtstride/way-farer.git"
  },
  "keywords": [
    "transport",
    "booking",
    "comfort",
    "travel",
    "transit",
    "bus",
    "public",
    "transportation",
    "move."
  ],
  "author": "godstime",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Gtstride/way-farer/issues"
  },
  "homepage": "https://github.com/Gtstride/way-farer#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.0.0",
    "chai": "^3.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.3",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "@types/express": "^4.17.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "brew": "0.0.8",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.3",
    "faker": "^4.1.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "moment": "^2.24.0",
    "node-pre-gyp": "^0.13.0",
    "pg": "^7.11.0",
    "postgresql": "0.0.1",
    "swagger-ui-express": "^4.0.7",
    "validate-js": "^2.0.1",
    "validator": "^11.1.0",
    "validatorjs": "^3.15.1"
  }
}
